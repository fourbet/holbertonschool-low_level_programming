#include "holberton.h"

/**
 * times_table - prints 9 times table
 *
 * Description: starting with 0
 *
 * Return: void
 */

void times_table(void)
{
	int i = 48;
	int j = 48;
	

	for ( i = 48 ; i < 57 ; i++)
	{
		for ( j = 48 ; j < 57 ; j++)
		{
			if ( i == 48)
				_putchar(48);
			if ( i == 49)
				_putchar(j);
			_putchar(',');
			_putchar(' ');
		}
		_putchar('\n');
	}
}
